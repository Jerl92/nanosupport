        "helpdesk",
        "support",
        "support desk",
        "support plugin",
        "support ticket",
        "ticket plugin",
        "ticket system",
        "ticketing system",
        "help desk",
        "wp support ticketing",
        "tickets",
        "help",
        "support staff",
        "support ticketing"
    ],
    "homepage": "http://nanosupport.nanodesignsbd.com",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Mayeenul Islam",
            "email": "wz.islam@gmail.com",
            "homepage": "http://nishachor.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "archive": {
        "exclude": [
            ".gitignore",
            "*.sublime-workspace",
            "*.sublime-project",
            "apigen.neon",
            "composer.json",
            "composer.lock",
            "README.md",
            "Gruntfile.js",
            "package.json",
            "/node_modules",
            "/tests",
            "!/vendor"
        ]
    },
    "support": {
        "email": "support@nanodesignsbd.com",
        "issues": "https://github.com/nanodesigns/nanosupport/issues",
        "wiki": "https://github.com/nanodesigns/nanosupport/wiki",
